{
	"authors": [
		"Agastya Das",
		"Heekyung Kim",
		"Roydon Pereira",
		"Jake Stringfellow",
		"Jiayue Zhao"
	],
	"configurations": [
		{
			"name": "Client",
			"mainSourceFile": "source/client.d",
			"targetName": "client",
			"targetType": "executable",
			"excludedSourceFiles": [
				"server/server.d",
				"source/test/*"
			]
		},
		{
			"name": "Server",
			"mainSourceFile": "server/server.d",
			"targetName": "server",
			"targetType": "executable",
			"excludedSourceFiles": [
				"source/client.d",
				"source/test/*"
			]
		},
		{
			"name": "unittest",
			"mainSourceFile": "source/client.d",
			"preBuildCommands": [
				"dub run unit-threaded --compiler=ldc2 -c gen_ut_main -- -f bin/ut.d -d dub"
			],
			"targetType": "executable",
			"dependencies": {
				"bindbc-sdl": "~>1.2.4",
				"bindbc-loader": "1.0.1",
				"unit-threaded": "~>2.1.5"
			}
		},
		{
			"name": "ddox",
			"mainSourceFile": "source/client.d",
			"sourceFiles": ["server/server.d"],
			"targetType": "executable",
			"dependencies": {
				"ddox": "~>0.16.22"
			},
			"excludedSourceFiles": [
				"source/test/*"
			]
		}
		
	],
	"copyright": "Copyright Â© 2023, Agastya Das, Heekyung Kim, Roydon Pereira, Jake Stringfellow, Jiayue Zhao",
	"dependencies": {
		"bindbc-sdl": "~>1.2.4",
		"dfmt": "~>0.15.0-beta.2"
	},
	"description": "Collaborative Drawing Application.",
	"license": "proprietary",
	"name": "graphics_project",
	
	"targetPath": "bin",
	"targetType": "executable"
}